<h2>Counting from 1 to 100:</h2>
@for (int i = 1; i <  101; i++)
{
    <h3>@i</h3>
}

<h2>Multiple of 3 and 5 less than 50:</h2>
@for (int i = 1; i <  50; i++)
{
    if (i % 3 == 0 && i % 5 ==0)
    {
       <h3>@i</h3>  
    }
   
}
<h2>Multiplication table of 19:</h2>
@for(int i = 1; i < 11;  i++)
{
   int a = 19;
   int multiply = i * a;
   <h3>@a* @i = @multiply</h3>  
}







